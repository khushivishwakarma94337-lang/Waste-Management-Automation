<!DOCTYPE html>
<html>
<head>
  <style>
    body {
      font-family: Arial, sans-serif;
      background: #f5f5f5;
      padding: 20px;
    }

    h1 {
      text-align: center;
      margin-bottom: 30px;
      color: #333;
    }

    h2 {
      margin-top: 30px;
      color: #444;
    }

    table {
      width: 80%;
      margin: auto;
      border-collapse: collapse;
      background: #fff;
      border-radius: 6px;
      overflow: hidden;
      box-shadow: 0 0 10px rgba(0,0,0,0.1);
    }

    th {
      background: #4CAF50;
      color: white;
      padding: 12px;
      font-size: 16px;
    }

    td {
      padding: 10px;
      text-align: center;
      border-bottom: 1px solid #ddd;
    }

    tr:hover {
      background: #f1f1f1;
    }

    a {
      display: block;
      width: 200px;
      margin: 30px auto;
      text-align: center;
      padding: 10px;
      background: #4CAF50;
      color: white;
      text-decoration: none;
      border-radius: 5px;
    }

    a:hover {
      background: #45a049;
    }
  </style>
</head>

<body>

<h1>Dashboard</h1>

<h2>Users</h2>
<table>
  <tr>
    <th>Name</th>
    <th>Email</th>
  </tr>

  <% users.forEach(u => { %>
    <tr>
      <td><%= u.name %></td>
      <td><%= u.location %></td>
    </tr>
  <% }) %>
</table>

<h2>Complaints</h2>
<table>
  <tr>
    
    <th>Complaint</th>
  </tr>

  <% complaints.forEach(c => { %>
    <tr>
        <td><%= c.name %></td>
      <td><%= c.query %></td>
    </tr>
  <% }) %>
</table>

<a href="/">Add More Data</a>

</body>
</html>